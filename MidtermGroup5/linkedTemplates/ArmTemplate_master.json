{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "MidtermGroup5"
		},
		"ls_adls_imdb_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_imdb'"
		},
		"ls_blob_imdb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_imdb'"
		},
		"ls_snowflake_imdb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_snowflake_imdb'"
		},
		"ls_adls_imdb_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://midtermadls5.dfs.core.windows.net/"
		},
		"ls_snowflake_imdb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "IMDB_DB"
		},
		"ls_snowflake_imdb_properties_typeProperties_role": {
			"type": "string",
			"defaultValue": "DEV_USER"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "MidtermGroup5_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_imdb_accountKey": {
						"value": "[parameters('ls_adls_imdb_accountKey')]"
					},
					"ls_blob_imdb_connectionString": {
						"value": "[parameters('ls_blob_imdb_connectionString')]"
					},
					"ls_snowflake_imdb_password": {
						"value": "[parameters('ls_snowflake_imdb_password')]"
					},
					"ls_adls_imdb_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_imdb_properties_typeProperties_url')]"
					},
					"ls_snowflake_imdb_properties_typeProperties_database": {
						"value": "[parameters('ls_snowflake_imdb_properties_typeProperties_database')]"
					},
					"ls_snowflake_imdb_properties_typeProperties_role": {
						"value": "[parameters('ls_snowflake_imdb_properties_typeProperties_role')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}