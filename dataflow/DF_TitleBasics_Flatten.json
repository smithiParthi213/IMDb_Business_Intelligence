{
	"name": "DF_Sample",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText_TitleBasics_tbl",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "AddidingDIColumns"
				},
				{
					"name": "flatten1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     job_Id as string (\"SP_21\")",
				"}",
				"source(output(",
				"          tconst as string,",
				"          titleType as string,",
				"          primaryTitle as string,",
				"          originalTitle as string,",
				"          isAdult as integer,",
				"          startYear as short,",
				"          endYear as short,",
				"          runtimeMinutes as short,",
				"          genres as string,",
				"          yearStatus_Flag as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 derive(genres = split(genres, ','),",
				"          DI_JOB_ID = $job_Id,",
				"          DI_LOAD_DT = currentDate()) ~> AddidingDIColumns",
				"AddidingDIColumns foldDown(unroll(genres),",
				"     mapColumn(",
				"          tconst,",
				"          genres,",
				"          DI_JOB_ID,",
				"          DI_LOAD_DT",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten1"
			]
		}
	}
}