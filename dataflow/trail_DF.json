{
	"name": "trail_DF",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText_Bronze_001",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "snf_silver_002",
						"type": "DatasetReference"
					},
					"name": "sinkSilver002"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     job_id as string (\"SP-21\"),",
				"     columnMappingJSON as string",
				"}",
				"source(output(",
				"          {titleId     ordering     title     region     language     types     attributes     isOriginalTitle} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     limit: 1000,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 derive(DI_JOB_ID = $job_id,",
				"          DI_LOAD_DT = currentDate()) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          DI_JOB_ID,",
				"          DI_LOAD_DT",
				"     )) ~> sinkSilver002"
			]
		}
	}
}